; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Household Helper"
config/description="Productivity suite for individuals and groups"
config/version="3.0.0.0"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://images/HH Icon.ico"
config/macos_native_icon="res://images/HH Icon.ico"
config/windows_native_icon="res://images/HH Icon.ico"

[autoload]

DataGlobal="*res://data/globals/data_global.gd"
SignalBus="*res://data/globals/signal_bus.gd"
gd="*res://addons/godot-action/utils_gd_action.tscn"
JsonSaveManager="*res://data/globals/json_save_manager.gd"
MainSettings="*res://data/globals/main_settings.gd"
TaskTrackingGlobal="*res://data/globals/task_tracking_global.gd"

[debug]

gdscript/warnings/unused_variable=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/energy_saving/keep_screen_on=false
window/stretch/aspect="ignore"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-action/plugin.cfg")

[gui]

theme/custom="res://theme/main_theme.tres"
theme/custom_font="res://fonts/font_otf/Gidolinya-Regular.otf"

[rendering]

textures/vram_compression/import_etc2_astc=true
